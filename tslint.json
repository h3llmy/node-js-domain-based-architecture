{
  "defaultSeverity": "error",
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "max-line-length": [true, 130],
    "prefer-template": true,
    "interface-name": [true, "always-prefix"],
    "import-name": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "class-name": true
  },
  "rulesDirectory": []
}
